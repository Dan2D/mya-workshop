{
  "parser": "babel-eslint",
  "extends": [
      "standard",
      "plugin:react/recommended"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "globals": {
      "fetch": false,
      "it": false,
      "localStorage": false
  },
  "rules": {
    "react/display-name": "off"
  }
}